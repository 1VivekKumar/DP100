						"value": "[parameters('linkedService2_properties_typeProperties_hostSubscriptionId')]"
					},
					"linkedService2_properties_typeProperties_clusterResourceGroup": {
						"value": "[parameters('linkedService2_properties_typeProperties_clusterResourceGroup')]"
					},
					"linkedService2_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('linkedService2_properties_typeProperties_servicePrincipalId')]"
					},
					"linkedService2_properties_typeProperties_tenant": {
						"value": "[parameters('linkedService2_properties_typeProperties_tenant')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'datafactoryvkvkv1_ArmTemplate_0')]"
